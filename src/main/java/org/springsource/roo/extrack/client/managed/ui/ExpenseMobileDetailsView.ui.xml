<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="amount">
            <div class="{style.label}">Amount</div>
            <div class="{style.value}" ui:field="amount"/>
        </div>
    <div id="description">
            <div class="{style.label}">Description</div>
            <div class="{style.value}" ui:field="description"/>
        </div>
    <div id="report">
            <div class="{style.label}">Report</div>
            <div class="{style.value}" ui:field="report"/>
        </div>
    <div id="approval">
            <div class="{style.label}">Approval</div>
            <div class="{style.value}" ui:field="approval"/>
        </div>
    <div id="category">
            <div class="{style.label}">Category</div>
            <div class="{style.value}" ui:field="category"/>
        </div>
    <div id="created">
            <div class="{style.label}">Created</div>
            <div class="{style.value}" ui:field="created"/>
        </div>
    <div id="reasonDenied">
            <div class="{style.label}">Reason Denied</div>
            <div class="{style.value}" ui:field="reasonDenied"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
