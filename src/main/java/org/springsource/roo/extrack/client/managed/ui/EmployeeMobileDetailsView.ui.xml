<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="displayName">
            <div class="{style.label}">Display Name</div>
            <div class="{style.value}" ui:field="displayName"/>
        </div>
    <div id="userName">
            <div class="{style.label}">User Name</div>
            <div class="{style.value}" ui:field="userName"/>
        </div>
    <div id="department">
            <div class="{style.label}">Department</div>
            <div class="{style.value}" ui:field="department"/>
        </div>
    <div id="supervisor">
            <div class="{style.label}">Supervisor</div>
            <div class="{style.value}" ui:field="supervisor"/>
        </div>
    <div id="gender">
            <div class="{style.label}">Gender</div>
            <div class="{style.value}" ui:field="gender"/>
        </div>
    <div id="admin">
            <div class="{style.label}">Admin</div>
            <div class="{style.value}" ui:field="admin"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
